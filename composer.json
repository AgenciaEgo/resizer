{
    "name": "ekersten/resizer",
    "description": "Image resizer for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Kersten",
            "email": "ekersten@gmail.com"
        }
    ],
    "homepage": "https://github.com/ekersten/resizer",
    "keywords": ["Laravel", "Resizer", "Image"],
    "require": {
        "illuminate/support": "~5",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ekersten\\Resizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ekersten\\Resizer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ekersten\\Resizer\\ResizerServiceProvider"
            ],
            "aliases": {
                "Resizer": "Ekersten\\Resizer\\Facades\\Resizer"
            }
        }
    }
}
